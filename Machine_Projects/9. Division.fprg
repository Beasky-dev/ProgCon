    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="Num1" type="Integer" array="False" size=""/>
            <declare name="Num2" type="Integer" array="False" size=""/>
            <declare name="Result" type="Integer" array="False" size=""/>
            <declare name="Answer" type="String" array="False" size=""/>
            <declare name="Again" type="String" array="False" size=""/>
            <declare name="ExitCode" type="Boolean" array="False" size=""/>
            <assign variable="ExitCode" expression="true"/>
            <output expression="&quot;Hi there, Welcome to Division of 2 Given Values&quot;" newline="True"/>
            <while expression="ExitCode == true">
                <output expression="&quot;Ready to DIVIDE from 2 GIVEN VALUES?&quot;" newline="True"/>
                <output expression="&quot;Note: This is a case sensitive program&quot;" newline="True"/>
                <output expression="&quot;Therefore, typing (yes) as a response to continue is neccessary&quot;" newline="True"/>
                <assign variable="Answer" expression="&quot;Yes&quot;"/>
                <input variable="Answer"/>
                <if expression="Answer ==&quot;yes&quot;">
                    <then>
                        <output expression="&quot;Enter dividend/numerator&quot;" newline="True"/>
                        <input variable="Num1"/>
                        <output expression="&quot;Enter divisor/denominator&quot;" newline="True"/>
                        <input variable="Num2"/>
                        <assign variable="Result" expression="Num1 / Num2"/>
                        <output expression="&quot;The answer is: &quot; &amp;result" newline="True"/>
                    </then>
                    <else>
                        <output expression="&quot;Error. Please try again.&quot;" newline="True"/>
                    </else>
                </if>
                <output expression="&quot;Restart?&quot;" newline="True"/>
                <output expression="&quot;Type 'yes' to restart&quot;" newline="True"/>
                <assign variable="Again" expression="&quot;yes&quot;"/>
                <input variable="Again"/>
                <if expression="Again ==&quot;yes&quot;">
                    <then>
                        <call expression="Main"/>
                    </then>
                    <else>
                        <output expression="&quot;Error. Please try again.&quot;" newline="True"/>
                    </else>
                </if>
            </while>
        </body>
    </function>
</flowgorithm>
